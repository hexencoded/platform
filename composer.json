{
    "name": "thor/framework",
    "description": "Thor CMS base & support package",
    "keywords": ["laravel", "cms", "framework", "thorcms", "multilingual", "crud"],
    "license": "MIT",
    "authors": [
        {
            "name": "Javier Aguilar",
            "homepage": "http://www.mjolnic.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "illuminate/support": "4.1.*",
        "thor/models": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "laravel/framework": "4.1.*"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-0": {
            "Thor": ["src"]
        },
        "psr-4": {
            "Thor\\": ["tests", "tests/testcases"]
        }
    },
    "minimum-stability": "dev"
}
