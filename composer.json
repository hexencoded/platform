{
    "name": "thorfw/core",
    "description": "Thor Framework core package",
    "keywords": ["laravel", "cms", "framework", "thor", "thorfw"],
    "authors": [
        {
            "name": "Javier Aguilar",
            "homepage": "http://www.mjolnic.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*",
        "thorfw/i18n": "1.0.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "files": [
            "src/Thor/helpers.php"
        ],
        "psr-0": {
            "Thor": "src/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "stable"
}
